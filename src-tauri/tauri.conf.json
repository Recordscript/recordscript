{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "screen-audio-recorder-test",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "fs": {
        "readFile": true,
        "scope": ["**"]
      }
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "screen-audio-recorder-test",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "ai.firstsupport.transcriber",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDU4MkJFOTNBQ0I4RTQ0OTMKUldTVFJJN0xPdWtyV0d2MjA1UUk4b1ZxcVNybHVuZmttS3ZXcGRhaFU2NUk2VXN4ZGp5NTNjSGgK",
      "endpoints": [
        "https://fs-transcriber.s3.ap-southeast-1.amazonaws.com/release.json"
      ],
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      }
    }
  }
}
